{
  "name": "playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4000",
    "build": "tsc && vite build",
    "lint": "eslint . --max-warnings 0",
    "format:write": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "gql-gen": "gql-tada generate output"
  },
  "dependencies": {
    "@apollo/client": "4.1.0",
    "@dnd-kit/core": "6.3.1",
    "@gsap/react": "2.1.2",
