    "@tanstack/react-router": "1.150.0",
    "@tanstack/zod-adapter": "1.150.0",
    "@wry/equality": "0.5.7",
    "gql.tada": "1.9.0",
    "graphql": "16.12.0",
    "gsap": "3.14.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-error-boundary": "6.1.0",
    "react-merge-refs": "3.0.2",
    "react-use-measure": "2.1.7",
    "rxjs": "7.8.2",
    "spin-delay": "2.0.1",
